{
  "name": "totamexcepturi",
  "version": "1.0.0",
  "description": "Essential utilities for TypeScript projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JackyPollich/totamexcepturi.git"
  },
  "funding": "https://github.com/sponsors/sindresorhus",
  "author": "RosendoRosenbaum",
  "type": "module",
  "exports": "./distribution/index.js",
  "types": "./distribution/index.d.ts",
  "engines": {
    "node": ">=12.20"
  },
  "files": [
    "distribution"
  ],
  "keywords": [
    "typescript",
    "ts",
    "type",
    "atque",
    "hic",
    "illo",
    "util",
    "quia",
    "extras",
    "guard",
    "guards",
    "labore"
  ],
  "dependencies": {
    "type-fest": "^2.5.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "ava": "^3.15.0",
    "del-cli": "^4.0.1",
    "expect-type": "^0.13.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "xo": "^0.45.0"
  },
  "sideEffects": false,
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/JackyPollich/totamexcepturi.git/issues"
  }
}